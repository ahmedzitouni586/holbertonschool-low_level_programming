#include "holberton.h"
/**
 * _strncpy - copies a string
 * @dest: number one 
 * @src: number two
 * @n: number three
 * Return: dest
 */
char *_strncpy(char *dest, char *src, int n)
{
  int i;
  for (i = 0; dest[i] != '\0'; i++)
    {
      src[i] = dest[i];
    }
  src[i] = '\0';
  return (0):
}
